{
  "buildCommand": "npm run build",
  "outputDirectory": ".svelte-kit/output/client",
  "framework": "sveltekit",
  "functions": {
    "src/routes/api/**.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
